<?xml version="1.0" encoding="utf-8"?>
<STOCK_ITEMS>
  <STOCK_ITEM>
	<!--Comment: main stock code -->
    <STOCK_CODE />
    <!--Comment: Optional. Alternative stock code set in Other Ref field. -->
    <ASSOCIATED_REF />
    <!--Comment: Short description / name -->
    <STOCK_DESC />
	<!--Comment: Optional extended/long description -->
	<LONG_DESC />
	<!--Comment: Optional EPOS description for item -->
 	<EPOS_DESC />
	<!--Comment: Optional additional web content for stock item -->
    <WEB_TEASER />
	<BUY_PRICE>2.48</BUY_PRICE>
    <BUY_PRICE_NET>0</BUY_PRICE_NET>
    <SELL_PRICE>5.99</SELL_PRICE>
    <SELL_PRICE_NET>0</SELL_PRICE_NET>
	<!--Comment: Used to override the sell price for the web -->
 	<SELL_PRICE_WEB></SELL_PRICE_WEB>
    <!--Comment: Deprecated, use STOCK_IMAGES below -->
	<IMAGE_FILENAME />
    <!--Comment: 20, 0, 15, ETC-->
    <VAT_RATE />
	<!--Comment: Country-specific VAT Rates for stock code - where applicable -->
    <COUNTRY_RATES>
		<COUNTRY_RATE COUNTRY_CODE="GB" COUNTRY_NAME="England">17.5</COUNTRY_RATE>
		<COUNTRY_RATE />
    </COUNTRY_RATES>
    <!--Comment: Every stock code will have at least two levels of stock type (1 & 2). Mid Type and Sub Type (levels 3 & 4) are optional. Where stock items are categorised to only three layers, levels 1, 2 and 4 will be used. Mid Type (level 3) will only be used when all four levels are required. -->
	<!--Comment: (level 1) (parent/header) stock type - alphanumeric -->
    <STOCK_TTYPE EPOS_DESC="XYZ">Level one stock</STOCK_TTYPE>
    <!--Comment: Unique ID for level 1 stock type-->
    <STOCK_TTYPE_ID />
    <!--Comment: (level 2) stock type. Alphanumeric - generally a textual description eg. "Books" -->
    <STOCK_TYPE EPOS_DESC="ABC">Sample stock type</STOCK_TYPE>
    <!--Comment: Unique ID for level 2 stock type -->
    <STOCK_TYPE_ID />
    <!--Comment: (level 3) stock type. Alphanumeric, eg. "56K" -->
    <STOCK_MID_TYPE EPOS_DESC="XYZ">aaaaM</STOCK_MID_TYPE>
    <!--Comment: Unique ID for level 3 stock type -->
    <STOCK_MID_TYPE_ID />
    <!--Comment: duplicate of STOCK_SUB_TYPE, deprecated -->
    <SUB_TYPE EPOS_DESC="XYZ" />
    <!--Comment: duplicate of STOCK_SUB_TYPE_ID, deprecated -->
    <SUB_TYPE_ID />
    <!--Comment: (level 4) stock type. Alphanumberic, eg. "56K" -->
    <STOCK_SUB_TYPE EPOS_DESC="XYZ">L4 type</STOCK_SUB_TYPE>
    <!--Comment: Unique ID for level 4 stock type -->
    <STOCK_SUB_TYPE_ID />
    <!--Comment: Unique Khaos Control ID for stock item -->
    <STOCK_ID />
	<!--Comment: Stock code's manufacturer. Optonal. -->
    <MANUFACTURER />
    <!--Comment: Preferred supplier's stock code, when one is defined. Superceded by SUPPLIER_STOCK_CODES section below-->	
    <SUPPLIER_STOCK_CODE />
    <!--Comment: lead time in days which this item normally takes to arrive when purchased -->
    <LEAD_TIME />
	<!--Comment: Average weight for the stock item. Please refer to customer with regards to whether this is in KG, Grammes, LBs etc. -->
    <WEIGHT />
    <!--Comment: Calculated tag that should be used to control whether or not the item is displayed on the customer's website. -->
	<!--Comment: Combines "web" and "discontinued" stock options -->
	<!--Comment: DELETED products should not be sold online -->
	<!--Comment: 0=Not deleted, -1=Deleted item  -->
    <DELETED />
    <!--Comment: Property that can be used to control whether or not the stock code is published to web site. Superceded by the DELETED tag -->
	<!--Comment: Stock codes not published to web should not be sold online -->
	<!--Comment: -1=Publish to Web 0=Do Not Publish to Web -->
    <WEB>0</WEB>
    <!--Comment: Property that can be used to discontinue a stock code. Superceded by the DELETED tag -->
	<!--Comment: Discontinued stock items should not be sold. -->
	<!--Comment: -1=Discontinued 0=Not Discontinued, can be sold as normal -->
    <DISCONTINUED>0</DISCONTINUED>
	<!--Comment: Property that defines whether or not a stock code can be discounted. -->
	<!--Comment: -1=Discounts disabled 0=Discounts Not Disabled -->
    <DISCOUNTS_DISABLED>0</DISCOUNTS_DISABLED>
	<!--Comment: Property that defines if the item should be sold or not when out of stock -->
	<!--Comment: -1=Do not sell once out of stock 0=Can be sold when out of stock -->
    <RUN_TO_ZERO>0</RUN_TO_ZERO>
	<!--Comment: Property that defines if the item is drop-shipped -->
	<!--Comment: -1=Item is drop-shipped 0=Item is not drop-shipped, as standard -->
    <DROP_SHIP>0</DROP_SHIP>
	<!--Comment: Optional. Property that can be used to define the order in which the product should appear on a webpage -->
    <WEB_PAGEORDER>1</WEB_PAGEORDER>
	<!--Comment: Optional. Colour value as defined in [ Stock | Detail | Telesales / Internet | Secondary ] screen in Khaos Control. -->
    <WEB_COLOURVALUE>xyz</WEB_COLOURVALUE>
    <!--Comment: Whether item is stock controlled or not (0=false, -1=true) -->
    <STOCK_CONTROLLED>0</STOCK_CONTROLLED>
    <!--Comment: SEO data -->
    <META_TITLE></META_TITLE>
    <META_DESCRIPTION></META_DESCRIPTION>
    <META_KEYWORDS></META_KEYWORDS>
    <!--Comment: Short description used on EPOS tills -->
    <EPOS_DESC>xxxx</EPOS_DESC>
    <!--Comment: Availability text for item -->          
    <AVAILABILITY></AVAILABILITY>
    <!--Comment: Max Display Qty (Integer), can be used to limit the amount of stock displayed -->
    <MAX_DISPLAY_QTY>0</MAX_DISPLAY_QTY>
	<!--Comment: Optional. Date that the product is to be launched. Can be used to control the date on which an item is displayed for sale -->
    <LAUNCH_DATE>2008-12-31</LAUNCH_DATE>
    <!--Comment: Optional. The number of reward points customers may earn when purchasing the item. If this is to be used, please discuss with KSDL first. -->
    <REWARD_POINTS></REWARD_POINTS>
    <!--Comment: Optional. User defined stock options -->
    <OPTION NAME="Specialist Range">0</OPTION>
    <OPTION NAME="Required Reading">0</OPTION>
    <!--Comment: Optional. User defined attributes (UDAs) -->
    <USER_DEFINED NAME="Author">Margaret Weis / Tracy Hickman</USER_DEFINED>
    <USER_DEFINED NAME="Date Published">2008-12-31</USER_DEFINED>
    <USER_DEFINED NAME="Heavy">0</USER_DEFINED>
    <USER_DEFINED NAME="ISBN-10">0786915749</USER_DEFINED>
    <USER_DEFINED NAME="ISBN-13">978-0786915743</USER_DEFINED>
    <!--Comment: Optional. List of restrictions/bars on this item (limits on where it can be sold -->
    <BARS>
		<!--Comment: The bar type indicates what type of restriction is imposed. -->
		<!--Comment: The only valid value currently is 1 = barred from country -->
		<!--Comment: The tag contents show the specific bar in question, eg. for countries, the 3 letter country code which is barred -->
		<BAR TYPE="1">JPN</BAR>
		<BAR TYPE="1">DEU</BAR>
    </BARS>
    <!--Comment: Optional. List of barcodes stored in Khaos Control for the stock item -->
	<BARCODES>
		<!--Comment: Type of barcode, when defined, is included as the DESC attribute -->
		<BARCODE DESC="ASIN">ABCDEFG</BARCODE>
		<BARCODE DESC="EAN13">1234567890123</BARCODE>
		<BARCODE />
    </BARCODES>
    <!--Comment: Optional. List of external files linked to stock item -->
    <EXTERNAL_DATA>
		<EXTERNAL_ITEM>
			<EXTERNAL_TYPE>1</EXTERNAL_TYPE>
			<EXTERNAL_VALUE>C:\File.txt</EXTERNAL_VALUE>
			<EXTERNAL_SHORT_DESC>My File Description</EXTERNAL_SHORT_DESC>
			<EXTERNAL_SORT_ORDER>1</EXTERNAL_SORT_ORDER>
		</EXTERNAL_ITEM>
		<EXTERNAL_ITEM>
			<EXTERNAL_TYPE>1</EXTERNAL_TYPE>
		</EXTERNAL_ITEM>
    </EXTERNAL_DATA>
    <!--Comment: Optional. List of images linked to stock item -->
    <STOCK_IMAGES>
		<STOCK_IMAGE>
			<IMAGE_NAME>Image Name</IMAGE_NAME>
			<IMAGE_DESC>Desctiption of the image</IMAGE_DESC>
			<FILE_NAME>C:\TestImage.jpg</FILE_NAME>
			<IMAGE_TYPE>Thumbnail</IMAGE_TYPE>
		</STOCK_IMAGE>
		<STOCK_IMAGE>
			<IMAGE_NAME>Image Name</IMAGE_NAME>
		</STOCK_IMAGE>
    </STOCK_IMAGES>
    <!--Comment: Optional. List of other stock items linked to the stock item -->
	<!--Comment: Please note that when a Pack Header item is included in a Sales Order XML file and imported into Khaos Control, the linked child items will be automatically added to the sales order that is created in Khaos Control -->
	<LINKED_ITEMS>
		<LINK_ITEM>
			<!--Comment: stock code for related item -->
			<STOCK_CODE />
			<!--Comment: 1=Upsell, 2=Crosssell, 3=Pack, 4=Associated -->
			<LINK_TYPE />
			<!--Comment: For U/C sell, the special price; for pack items, the quantity contained -->
			<LINK_VALUE />
			<!--Comment: Should this item be automatically added to the sales order 0=No, -1=Yes-->
			<AUTO_ADD />
		</LINK_ITEM>
		<LINK_ITEM>
			<!--Comment: further links follow... -->
		</LINK_ITEM>
    </LINKED_ITEMS>
	<!--Comment: Optional. List of supplier stock codes, and associated information, for the stock item. Supercedes SUPPLIER_STOCK_CODE above -->
	<SUPPLIER_STOCK_CODES>
		<!--Comment: The supplier stock code for the item -->
		<!--Comment: IS_PREFERRED attribute marks whether or not the supplier is preferred. -1=Yes 0=No -->
		<!--Comment: SUPPLIER_NAME attribute returns the Company Name for the supplier -->
		<SUPPLIER_REF IS_PREFERRED="-1" SUPPLIER_NAME="SOME SUPPLIER">00012A</SUPPLIER_REF>
		<SUPPLIER_REF IS_PREFERRED="0" SUPPLIER_NAME="SOME OTHER SUPPLIER">221012A</SUPPLIER_REF>
	</SUPPLIER_STOCK_CODES>
	<!--Comment: Optional. Details the Size Colour Style profile for the stock item, when defined -->
	<!--Comment: File structure for SCS items:
	SCS (Size/Colour/Style variants) can either be exported as completely separate items
	(in which case they get their own STOCK_ITEM tag, and aren't directly 'linked together')
	or they can be exported with the structure intact, in which case there is one STOCK_ITEM
	tag for the 'parent item'/'template', with an SCS tag containing the hierarchy of child
	items.
	-->
    <SCS>
		<!--Comment: The headings indicate what data each 'level' within the template contains -->
		<HEADING LEVEL="1">Size</HEADING>
		<HEADING LEVEL="2">Colour</HEADING>
		<HEADING LEVEL="3">Style</HEADING>
		<HEADING LEVEL="4">Length</HEADING>
		<!--Comment: styles can be up to four levels deep with an SCS item at the lowest level -->
		<!--Comment: tags all have same format and use as tags in main item above -->
		<STYLE>
			<DESCRIPTION />
			<!--Comment: stock code suffix for this element (already included in STOCK_CODE - just separated out for information) -->
			<SCS_CODE_SUFFIX>ABC</SCS_CODE_SUFFIX>
			<SCS_ITEM>
				<STOCK_CODE />
				<ASSOCIATED_REF />
				<DESCRIPTION />
				<WEIGHT />
				<!--Comment: The IS_NET tags for the Buy and Sell prices are not included for SCS children as the value from the parent should be inherited.-->
				<BUY_PRICE />
				<SELL_PRICE />
				<SELL_PRICE_WEB />
				<STOCK_ID />
				<LONG_DESC />
				<LAUNCH_DATE>2008-12-31</LAUNCH_DATE>
				<LEAD_TIME />
				<SUPPLIER_STOCK_CODE />
				<IMAGE_FILENAME />
				<DELETED />
				<WEB>0</WEB>
				<DISCONTINUED>0</DISCONTINUED>
				<DROP_SHIP>0</DROP_SHIP>
				<DISCOUNTS_DISABLED>0</DISCOUNTS_DISABLED>
				<RUN_TO_ZERO>0</RUN_TO_ZERO>
				<WEB_PAGEORDER>1</WEB_PAGEORDER>
				<WEB_COLOURVALUE>xyz</WEB_COLOURVALUE>
				<STOCK_CONTROLLED>0</STOCK_CONTROLLED>
				<EPOS_DESC>xxxx</EPOS_DESC>       
				<META_TITLE></META_TITLE>
				<META_DESCRIPTION></META_DESCRIPTION>
				<META_KEYWORDS></META_KEYWORDS>
				<AVAILABILITY></AVAILABILITY>
				<REWARD_POINTS></REWARD_POINTS>
				<MAX_DISPLAY_QTY>0</MAX_DISPLAY_QTY>
				<VAT_RATE />
				<COUNTRY_RATES>
					<COUNTRY_RATE COUNTRY_CODE="GB" COUNTRY_NAME="England">17.5</COUNTRY_RATE>
					<COUNTRY_RATE />
				</COUNTRY_RATES>
				<STOCK_TYPE EPOS_DESC="ABC">Sample stock type</STOCK_TYPE>
				<STOCK_TTYPE EPOS_DESC="XYZ">Level one stock</STOCK_TTYPE>
				<STOCK_MID_TYPE EPOS_DESC="XYZ">aaaaM</STOCK_MID_TYPE>
				<STOCK_SUB_TYPE EPOS_DESC="XYZ">L4 type</STOCK_SUB_TYPE>
				<SUB_TYPE_ID></SUB_TYPE_ID>
				<!--Comment: SCS stock item related items -->
				<LINKED_ITEMS>
					<LINK_ITEM>
						<STOCK_CODE />
						<LINK_TYPE />
						<LINK_VALUE />
					</LINK_ITEM>
					<LINK_ITEM />
				</LINKED_ITEMS>
				<STOCK_IMAGES>
					<STOCK_IMAGE>
						<IMAGE_NAME>Image Name</IMAGE_NAME>
						<IMAGE_DESC>Desctiption of the image</IMAGE_DESC>
						<FILE_NAME>C:\TestImage.jpg</FILE_NAME>
						<IMAGE_TYPE>Thumbnail</IMAGE_TYPE>
					</STOCK_IMAGE>
					<STOCK_IMAGE>
						<IMAGE_NAME>Image Name</IMAGE_NAME>
					</STOCK_IMAGE>
				</STOCK_IMAGES>
				<BARCODES>
					<BARCODE>ABCDEFG</BARCODE>
					<BARCODE />
				</BARCODES>
			</SCS_ITEM>
        </STYLE>
		<STYLE>
			<DESCRIPTION />
        </STYLE>
        <STYLE>
			<DESCRIPTION />
        </STYLE>
		<STYLE>
			<DESCRIPTION />
		</STYLE>
    </SCS>
  </STOCK_ITEM>
  <STOCK_ITEM>
    <!--Comment: further items follow... -->
  </STOCK_ITEM>
</STOCK_ITEMS>